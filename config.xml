<widget xmlns			= "http://www.w3.org/ns/widgets"
		xmlns:gap       = "http://phonegap.com/ns/1.0"
		xmlns:android   = "http://schemas.android.com/apk/res/android"
        id			= "sw.afcorredor.cineapp"
        version			= "0.3.6">
    <name>CineApp</name>

    <description>
    </description>
<!--
    <author href="http://cordova.io" email="callback-dev@incubator.apache.org">
        Apache Cordova Team
    </author>
	-->

	<preference name="phonegap-version" value="3.5.0" /> 
    <preference name="ios-statusbarstyle"         value="black-opaque" />
	
    <access origin="*.apache.org"/>

    <!-- <content src="http://mysite.com/myapp.html" /> for external pages -->
    <content src="index.html" />

    <log level="DEBUG"/>

        <!-- Preferences for Android -->
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="true" />
    <preference name="EnableViewportScale" value="true" />
    
    <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
    <gap:plugin name="org.apache.cordova.file" version="1.3.1" />
    <gap:plugin name="org.apache.cordova.file-transfer" version="0.4.6" />
    <gap:plugin name="org.apache.cordova.media" version="0.2.13" />
    <gap:plugin name="nl.x-services.plugins.insomnia" version="4.0.1" />
    <gap:plugin name="com.badrit.backbutton" version="0.1.0" />
    <gap:plugin name="net.yoik.cordova.plugins.screenorientation" version="1.3.1" />
    <plugin name="Media" value="org.apache.cordova.AudioHandler" />
    <!-- <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" version="1.2.0" /> -->
    <gap:plugin name="com.phonegap.plugin.mobile-accessibility" version="0.1.1" />
    
<!--     <gap:plugin name="com.phonegap.plugin.mobile-accessibility" version="0.1.1" /> -->
    <!-- IOS 
    <plugin name="File" value="CDVFile" />
    <plugin name="FileTransfer" value="CDVFileTransfer" />
    <plugin name="Media" value="CDVSound" />
    -->
    <icon src="images/icons/144.png" />


    <icon src="images/icons/57.png"          gap:platform="ios" width="57"  height="57"  />
    <icon src="images/icons/72.png"       gap:platform="ios" width="72"  height="72"  />
    <icon src="images/icons/114.png"    gap:platform="ios" width="114" height="114" />
    <icon src="images/icons/144.png" gap:platform="ios" width="144" height="144" />

    <icon gap:platform="ios" height="57" src="images/icons/57.png" width="57" />
    <icon gap:platform="ios" height="72" src="images/icons/72.png" width="72" />
    <icon gap:platform="ios" height="114" src="images/icons/114.png" width="114" />
    <icon gap:platform="ios" height="144" src="images/icons/144.png" width="144" />

    <icon gap:platform="ios" height="57" src="images/icons/transmetroIcon.png" width="57" />
    <icon gap:platform="ios" height="72" src="images/icons/transmetroIcon.png" width="72" />
    <icon gap:platform="ios" height="114" src="images/icons/transmetroIcon.png" width="114" />
    <icon gap:platform="ios" height="144" src="images/icons/transmetroIcon.png" width="144" />
    <icon gap:platform="winphone" gap:role="background" src="images/icons/golombiaoIcon.png" />
    


    <icon gap:platform="ios" height="57" src="images/icons/transmetroIcon.png" width="57" />
    <icon gap:platform="ios" height="72" src="images/icons/transmetroIcon.png" width="72" />
    <icon gap:platform="ios" height="114" src="images/icons/transmetroIcon.png" width="114" />
    <icon gap:platform="ios" height="144" src="images/icons/transmetroIcon.png" width="144" />
    <icon gap:platform="winphone" gap:role="background" src="images/icons/golombiaoIcon.png" />

